name=\"email\"\n                    label=\"Email\"\n                    inputRef={register}\n                    error={!!errors.email}\n                    helperText={errors.email?.message}\n                />\n                <StyledInput\n                    name=\"bio\"\n                    label=\"Biografia\"\n                    inputRef={register}\n                    error={!!errors.bio}\n                    helperText={errors.bio?.message}\n                />\n                <StyledInput\n                    name=\"course_module\"\n                    label=\"Modulo\"\n                    inputRef={register}\n                    error={!!errors.course_module}\n                    helperText={errors.course_module?.message}\n                />\n                <StyledInput\n                    name=\"contact\"\n                    label=\"Contato\"\n                    inputRef={register}\n                    error={!!errors.contact}\n                    helperText={errors.contact?.message}\n                />\n                <StyledInput\n                    name=\"password\"\n                    label=\"Senha\"\n                    inputRef={register}\n                    error={!!errors.password}\n                    helperText={errors.password?.message}\n                />\n                <StyledInput\n                    name=\"password_confirmation\"\n                    label=\"Confirme a senha\"\n                    inputRef={register}\n                    error={!!errors.password_confirmation}\n                    helperText={errors.password_confirmation?.message}\n                />\n                <Button type=\"submit\">Enviar</Button>\n            </StyledForm>\n        </Container>\n    )\n}\n\nexport default RegisterPage","/media/samara/HD 1tb/02 - Q2 - kenzie/kenzie-hub/src/components/header/index.jsx",[],"/media/samara/HD 1tb/02 - Q2 - kenzie/kenzie-hub/src/pages/home/index.jsx",[],"/media/samara/HD 1tb/02 - Q2 - kenzie/kenzie-hub/src/s